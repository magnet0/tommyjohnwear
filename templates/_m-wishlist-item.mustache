<div class="m-wishlist-item">
	<div class="wishlist-remove">
		<i class="btn-round">&times;</i>
	</div>
	<div class="wishlist-img">
		<img src="{{ image.thumb }}" alt="">
	</div>
	<div class="wishlist-product-info">
		<h2 class="wishlist-name">{{ name }} <small class="wishlist-category">{{ category }}</small></h2>
		<p>{{ quantity }} &times; Size: {{ size }}</p>
	</div>
	<div class="wishlist-button">
		<button type="button">Add to Cart</button>
	</div>
</div>