{{> head }}
		
<article role="main">
    <header class="stage">
        
        <div class="l-wrapper">
            <h1 class="heading">{{stage.heading}}</h1>
            <p class="lead">{{stage.lead}}</p>
        </div>
        
    </header>
    <div class="content">

        <div class="l-wrapper">
            <div class="l-grid-six">
                <div class="m-feature-row">
                    <div class="m-feature-image l-block-third"></div>
                    <div class="l-block-twothird">
                        <p></p>
                    </div>
                </div>
                <div class="m-feature-row">
                    <div class="m-feature-image l-block-third"></div>
                    <div class="l-block-twothird">
                        <p></p>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="m-primary-feature m-feature l-wrapper">
            <div class="l-grid-six">
                {{#primary-feature}}
                    <div class="l-block-third">
                        {{> m-feature}}
                    </div>
                {{/primary-feature}}
            </div>
        </div>
        
	</div>
</article>
		
{{> sidebar }}

